{
    "name": "inktweb/bolcom-retailer-api-laravel",
    "description": "Laravel bridge for inktweb/bolcom-retailer-api-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Inktweb",
            "email": "info@inktweb.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "inktweb/bolcom-retailer-api-php": "^2.0",
        "illuminate/support": "^9.0 | ^10.0 | ^11.0"
    },
    "autoload": {
        "psr-4": {
            "Inktweb\\Bolcom\\RetailerApi\\Laravel\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/inktweb/bolcom-retailer-api-php"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        },
        "laravel": {
            "providers": [
                "Inktweb\\Bolcom\\RetailerApi\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "BolcomRetailerApi": "Inktweb\\Bolcom\\RetailerApi\\Laravel\\Facade"
            }
        }
    }
}
